  HostName github.com
  User git
  IdentityAgent ~/Library/Group\ Containers/2BUA8C4S2C.com.1password/t/agent.sock
  IdentityFile ~/.ssh/github_nscale_moon_1pw.pub
  IdentitiesOnly yes

# Fallback
Host *
  IdentityAgent ~/Library/Group\ Containers/2BUA8C4S2C.com.1password/t/agent.sock
